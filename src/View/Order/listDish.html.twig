{% extends 'Order/index.html.twig' %}

{% block contentCard %}
    <div class="row order-box-card">
        {% for dish in dishs %}
            <div class="card col-8 col-md-3 order-card-dish">
                <img class="card-img-top" src="data:image/jpeg;base64," alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title">{{ dish.title }}</h5>
                    <p class="card-text">{{ dish.composition }}</p>
                    <div class="order-bottom-box">
                        <p class="card-text">{{ dish.price }}€</p>
                        <a href="/order/add/dish/{{ dish.type }}/{{ dish.id }}/" class="btn-order-card">Ajouter</a>
                    </div>
                </div>
            </div>
        {% else %}
            <p>Désolé, mais il n'y a rien dans cette section</p>
        {% endfor %}
    </div>
{% endblock contentCard %}